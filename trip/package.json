    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.7",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
