{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.12.3",
    "@fortawesome/fontawesome-free": "5.11.2",
    "@testing-library/jest-dom": "^5.11.4",
